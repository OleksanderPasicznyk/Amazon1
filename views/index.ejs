<html>
	<head>
		<link href="http://netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css" rel="stylesheet">	
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />	
	</head>
	<body>
		<nav class="navbar">
			
		</nav>
		<div class="container">
		<section id="section1" class="panel panel-default">

			<div class="panel-heading">
			<div class="container">
			<h2><a href="http://localhost:8080/list">List of pics</a></h2>
			</div>
		  	</div>
	  		<div class="panel-body">
	  <form action="https://<%=bucket%>.s3.amazonaws.com/" method="POST" id="sign-up-form" class="form-horizontal" enctype="multipart/form-data">	
	<!-- W tym miejscu zostaja wygenerowane pola formularza przez kod w pliku createform.js -->
	<div class="container">
	<% fields.forEach(function(elem){ %>
	  <input type="hidden" name="<%= elem.name  %>" value="<%= elem.value %>"/>
		<%}); %> 
	   
	<div class="col-xs-5">
	<div class="col-xs-5">
	<div class="form-group">
    	<label for="exampleInputFile">File input</label>	<!-- Przycisk dodawania pliku -->

    	<input type="file" accept=".jpg,.png" name="file">
  </div>
	
  	
	</div>
	</div>
	
	<div class="container">
	<div class="col-xs-11">
	<input type="submit" class="btn  btn-primary btn-block" value="Confirm"/> <!-- Przycisk wysylania -->
	</div>
	</div>
</form>
	  		</div>
			<footer class="panel-footer">
				
			</footer>
		</section>
	</div>		
	</body>
</html>
